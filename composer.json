{
    "name": "20tries/filterable",
    "description": "A package for supporting the filtering of controller action results.",
    "keywords": [
        "filter", "filterable", "laravel"
    ],
    "homepage": "https://github.com/20tries/filterable",
    "license": "MIT",
    "require": {
        "php" : ">=5.6",
        "nesbot/carbon": "1.*",
        "20tries/date_range": "0.0.*",
        "illuminate/support": "5.2.*",
        "illuminate/contracts": "5.2.*",
        "illuminate/http": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*",
        "symfony/var-dumper": "3.0.*",
        "symfony/debug": "3.0.*",
        "illuminate/database": "5.2.*"
    },
    "autoload": {
        "psr-4": {
            "_20TRIES\\Filterable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "_20TRIES\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        }
    }
}
