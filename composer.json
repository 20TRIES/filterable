{
    "name": "20tries/filterable",
    "description": "A package to simplify the process of filtering controller / api results.",
    "keywords": [
        "filter", "filterable", "filtering", "request", "query", "api", "request-to-query"
    ],
    "homepage": "https://github.com/20tries/filterable",
    "license": "MIT",
    "require": {
        "php" : ">=5.6",
        "illuminate/http": "5.3.*"
    },
    "require-dev": {
        "php" : ">=5.6",
        "phpunit/phpunit": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "_20TRIES\\Filterable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "_20TRIES\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        }
    }
}
